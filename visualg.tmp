algoritmo "calculadora_quantidade_de_produtos"

var
   minimo, maximo, medio: real

inicio
      escreval("calculadora quantidade da produtos")
      escreval("Valor mínimo")
      leia(minimo)

      escreval("valor máximo")
      leia(maximo)
      
//calcular a media

      medio <-(minimo*maximo)/2
      escreval("valor medio:", medio)
fimalgoritmo
