algoritmo "somar_numeros"

var
   num1, num2, resp : real
inicio
// Seção de Comandos 
   escreval("Calculadora para somar dois números")
   escreval("Insira o primeiro número")
   leia (num1)
   escreval("Insira o segundo número")
   leia (num2)
   resp <- num1 + num2
   escreval("O resultado da soma é ",resp)
fimalgoritmo
